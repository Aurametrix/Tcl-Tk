# match ip address

set a {example192.168.0.1example10.0.0.1example172.16.0.1}

set ipAddrs [regexp -inline -all {\d+\.\d+\.\d+\.\d+} $a]

set ipAddr [lindex $ipAddrs 1]

# Tcl lists index from 0, so the 5th IP address is [lindex $ipAddrs 4]

# imperfect solution:
regexp {.*?(\d+\.\d+\.\d+\.\d+).*?(\d+\.\d+\.\d+\.\d+)} $a match sub1 sub2
puts $sub2 = 10.0.0.1
# actually the leading .*? and parentheses will cause double match

